<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@taglib prefix="sql" uri="http://java.sun.com/jsp/jstl/sql" %>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Courseware</title>
        <%@include file="links4temp.jsp" %><%@include file="links4bsa.jsp" %>
    </head>
    <body>
        <%
            String cid = request.getParameter("id");
        %>
            <sql:setDataSource  var="databse" driver="com.mysql.jdbc.Driver" url="jdbc:mysql://localhost:3306/e_courseware?zeroDateTimeBehavior=convertToNull" user="root"></sql:setDataSource>

                 <sql:query var="course" dataSource="${databse}">
                     select * from courseware where c_id = '<%=cid%>'
            </sql:query> 
        
        <%@include file="mainheader.jsp" %>
        <c:forEach items="${course.rows}" var="crs">
        <div class="row-fluid">
                        <!-- block -->
                        <div class="block">
                            <div class="navbar navbar-inner block-header" >
                                <div class="muted pull-left">Courseware</div>
                                <div class="pull-right"><span class="badge badge-info"></span>

                                </div>
                            </div>
                            <div class="block-content collapse in">
                                <div class="row-fluid padd-bottom">
                                  
                                  <div class="span3">
                                      <a href="#" class="thumbnail">
                                        <img src="assetstemp\images\portfolio\img1.jpg" alt="260x180" style="width: 260px; height: 180px;" src="data:image/png;base64,">
                                      </a>
                                  
                                    
                                   <div class="form-actions" >
                                          <button type="submit" class="btn btn-primary" style="margin-left: 4%;"><a href="" style="color: white;">Enroll</a></button>
                                          <button type="submit" class="btn btn-primary " style="margin-left: 4%;"><a href="courses.jsp" style="color: white;">Return</a></button>
                                        </div></div>
                                    
                                    <div class="3 offset 2">
						<div class="row">
							<div class="col-md-6">
                                                            <h3>Course Profile:</h3>
                                                            <b> Course name:</b> ${crs.c_name}<br><br>
                                                            <b>Course code: </b> ${crs.c_id}<br><br>
                                                            <b> Duration: </b> ${crs.duration}<br><br>
                                                            <b> Available: </b>${crs.status}<br><br>
                                                            <b>Category:</b>Web design/Programming language<br><br>
                                                            <b>Details: ${crs.details}</b><br> <br>
                                                            <b>Guided By: ${crs.p_id}</b><br> <br>
                                                            <b>Starts On: ${crs.u_timestamp}</b>
                                                            
                                                            
                                                            
                                                            </div>
				</div>
			</div>
                                 
                                    
                                    
                                </div>
                            </div>
                        </div>
                        
                        
                        <%@include file="mainfooter.jsp" %>
                       
                    </div>     
        </c:forEach>
       <%@include file="script4temp.jsp" %><%@include file="script4bsa.jsp" %>
    </body>
</html>
